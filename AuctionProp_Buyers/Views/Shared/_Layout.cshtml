<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <title>Auction-Prop</title>
    <link rel="shortcut icon " type="image/png" href="~/Images/favicon.png" />

    <!--Bootstrap Link-->
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
   

</head>
<body>


    <!--Starting Header Navigation Bar-->
    <div class="navbar navbar-fixed-top">

        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="#"><img src="~/Images/logo.png" /></a>
                <button class="navbar-toggle" data-toggle="collapse" data-target=".navHeaderCollapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
               
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            Auctions <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Bank Instructed</a></li>
                            <li><a href="#">Residential Properties</a></li>
                            <li><a href="#">Commercial Properties</a></li>
                            <li><a href="#">Developments</a></li>
                        </ul>
                    <li><a href="">Buyers Info</a></li>
                    <li><a href="#">Sellers Info</a></li>
                    <li><a href="#">Contact Us</a></li>
                    <li><a href="#">T&C's</a></li>
                </ul>
          @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    
    <div class="container body-content">
        @RenderBody()
        <hr />



        <div class="footer">
            <footer>
                <div class="row">
                    <div class="col-m-4">



                    </div>
                    <div class="col-m-4">
                        <p>Copyright© 2020</p>


                    </div>
                    <div class="col-m-4">
                        <a class="" href="#"><img src="~/Images/logo.png" alt="logo" /></a>


                    </div>
                </div>
            </footer>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
